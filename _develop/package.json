{
  "name": "frontend-template",
  "version": "1.0.0",
  "description": "template",
  "main": "index.js",
  "engines": {
    "node": "^12.x"
  },
  "repository": {
    "type": "git",
    "url": "http://backlog.caters.jp:8969/backlog/git/FRONTEND/FrontEndTemplate-v2.git"
  },
  "license": "ISC",
  "author": "matsumoto",
  "browserslist": [
    "> 5% in alt-AS",
    "last 2 major versions",
    "iOS >= 9",
    "ie 11"
  ],
  "config_info": {
    "rootPath": "rootPathか否か。 相対でやりたい場合 src/pug/にディレクトリ作って npm run create:pug すると相対パスが解決されたpugが自動生成される",
    "dest": "書き出し先を指定。",
    "assetPath": "commonとかassetsとかのディレクトリ名。スラッシュ不要",
    "subDirectory": "サブディレクトリが有る場合指定。最初に/は要らない。例:public_html/lp/の場合はlp/。lp/01/なども可"
  },
  "config": {
    "dest": "../public_html/",
    "assetPath": "assets",
    "subDirectory": "iibe/"
  },
  "scripts": {
    "start": "run-s create:directory init",
    "init": "run-s clean:host & run-p watch:* & run-p validate:*",
    "clean:host": "lsof -n -i:3000 | grep LISTEN | awk '{print $2}' | xargs kill -9",
    "clean:js": "cross-conf-env rimraf \"npm_package_config_destnpm_package_config_subDirectorynpm_package_config_assetPath/js\"",
    "clean:cache": "rimraf \"./node_modules/.cache/\"",
    "create:directory": "mkdirp \"src/fonts\" & mkdirp \"src/images\"",
    "watch:pug": "cross-conf-env pug src/pug --out npm_package_config_destnpm_package_config_subDirectory --watch --pretty --hierarchy",
    "watch:css": "cross-conf-env postcss \"src/css/**/!(_)*.css\" -d \"npm_package_config_destnpm_package_config_subDirectorynpm_package_config_assetPath/css\" -c \"./postcss.config.js\" --env=production --ext css -w",
    "build:css": "cross-conf-env postcss \"src/css/**/!(_)*.css\" -d \"npm_package_config_destnpm_package_config_subDirectorynpm_package_config_assetPath/css\" -c \"./postcss.config.js\" --env=production --ext css",
    "watch:image": "node \"npm_scripts/watch_image\"",
    "build:image": "node \"npm_scripts/cpx_image\"",
    "build:html": "node \"npm_scripts/cache_busting\"",
    "__ignore_watch:icon": "chokidar \"src/fonts/**/*.svg\" -c \"npm run build:icon\"",
    "build:icon": "cross-conf-env mkdir \"npm_package_config_destnpm_package_config_subDirectorynpm_package_config_assetPath/fonts\" & cross-conf-env icon-font-generator \"./src/fonts/*.svg\" -o \"npm_package_config_destnpm_package_config_subDirectorynpm_package_config_assetPath/fonts/\" --csspath \"src/css/_config/_fonts.css\" -p glyphs --name glyphs --json false --mono true --cssfontsurl \"/npm_package_config_subDirectorynpm_package_config_assetPath/fonts\" --html false --types \"woff, woff2\" --normalize true --height 128 --center true",
    "watch:js": "npm run clean:cache & webpack -w --progress --colors --env.NODE_ENV=development",
    "build:js": "npm run clean:cache & webpack --progress --env.NODE_ENV=production",
    "build": "run-s build:js build:html",
    "watch:localhost": "browser-sync start --config bs-config.js",
    "validate:html": "cross-conf-env chokidar \"npm_package_config_destnpm_package_config_subDirectory**/*.html\" -c \"html-validate --config htmlvalidate.json {path}\"",
    "relative": "node \"npm_scripts/to_relative\""
  },
  "dependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@svgr/webpack": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "asset-cache-bust": "^2.2.0",
    "autoprefixer": "^9.8.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "browser-sync": "^2.26.12",
    "cheerio": "^1.0.0-rc.3",
    "chokidar-cli": "^2.1.0",
    "connect-history-api-fallback": "^1.6.0",
    "connect-ssi": "^1.1.1",
    "core-js": "^3.6.5",
    "cpx": "^1.5.0",
    "cross-conf-env": "^1.1.2",
    "css-declaration-sorter": "^5.1.2",
    "css-mqpacker": "^7.0.0",
    "csswring": "^7.0.0",
    "data.maybe": "^1.2.3",
    "data.task": "^3.1.2",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "event-hooks-webpack-plugin": "^2.1.7",
    "fetch-jsonp": "^1.1.3",
    "file-loader": "^6.0.0",
    "fingerprinting": "^1.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs": "^0.0.2",
    "gsap": "^3.4.2",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-validate": "^3.0.0",
    "htmlparser2": "^4.1.0",
    "icon-font-generator": "^2.1.10",
    "immutable-ext": "^1.1.5",
    "intersection-observer": "^0.11.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "path-to-regexp": "^6.1.0",
    "picturefill": "^3.0.3",
    "postcss": "^7.0.32",
    "postcss-cachebuster": "^0.1.6",
    "postcss-center": "^1.1.0",
    "postcss-cli": "^7.1.1",
    "postcss-comment": "^2.0.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-extend-rule": "^3.0.0",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-for": "^2.1.1",
    "postcss-functions": "^3.0.0",
    "postcss-gradient-transparency-fix": "^4.0.0",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.3",
    "postcss-nested": "^4.2.3",
    "postcss-normalize": "^9.0.0",
    "postcss-reporter": "^6.0.1",
    "postcss-simple-vars": "^5.0.2",
    "postcss-sorting": "^5.0.1",
    "postcss-will-change-transition": "^1.2.0",
    "prettier": "^2.0.5",
